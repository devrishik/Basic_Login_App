<ul class="breadcrumb">
    <li><a href="/">Home</a> <span class="divider">/</span></li>
    <li><a href="<%- pathTo.lists %>">lists</a> <span class="divider">/</span></li>
    <li class="active"><a href="#">Details of list</a></li>
</ul>

<div class="page-header">
    <h1>Details of list</h1>
</div>

<table class="table table-bordered">
    <tbody>
        <tr><th>dev</th><td><%= list.dev %></td></tr>
    </tbody>
</table>

<div class="well">
    <%- link_to('<i class="icon-edit icon-white"></i> Edit', pathTo.edit_list(list), {class: 'btn btn-primary'}) %>
    <%- link_to('<i class="icon-remove icon-white"></i> Delete', pathTo.list(list), {class: 'btn btn-danger', method: 'delete', remote: true, jsonp: '(function (u) { location.href = u; })'}) %>
    or
    <%- link_to('Back to index', pathTo.lists) %>
</div>
