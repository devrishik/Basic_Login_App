<ul class="breadcrumb">
    <li><a href="/">Home</a> <span class="divider">/</span></li>
    <li><a href="<%- pathTo.lists() %>">lists</a> <span class="divider">/</span></li>
    <li class="active"><a href="#">Edit list</a></li>
</ul>

<div class="page-header">
    <h1>Edit list</h1>
</div>

<% var form = formFor(list, {action: pathTo.list(list), method: 'PUT', id: "list-form", class: 'form-horizontal'}) %>
<%- form.begin() %>
<%- include _form %>
<div class="form-actions">
    <%- form.submit('<i class="icon-ok icon-white"></i> Update list', {class: 'btn btn-primary'}) %> or
    <%- linkTo('Cancel', pathTo.list(list), {class: 'btn'}) %>
</div>
<%- form.end() %>
